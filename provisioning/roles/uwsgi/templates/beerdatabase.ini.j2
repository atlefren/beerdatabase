[uwsgi]
plugin = python
workers = 5
threads = 2
pp = {{ app.root }}
chdir = {{ app.root }}
home = {{ app.root }}/venv
module = runapp
callable = app
env = SECRET_KEY={{flask_settings.secret_key}}
env = DEBUG={{flask_settings.debug}}
env = SQLALCHEMY_DATABASE_URI=postgresql://{{database.username}}:{{database.password}}@localhost:5432/{{database.name}}